#: ../ailurus/common/tips.py:28
msgid ""
"motto of Ailurus\n"
"Give a man a fish and he can eat for a day; but if you teach him how to "
"fish, he'll eat for a lifetime."
msgstr ""
"мото на Ailurus\n"
"Дай на човека риба и той ще яде ден; но ако научиш човека да лови риба, той "
"ще яде цял живот."

#: ../ailurus/common/tips.py:31
msgid ""
"Display Linux distributor's ID\n"
"lsb_release -is\n"
"\n"
"Display Linux release number\n"
"lsb_release -rs\n"
"\n"
"Display Linux code name\n"
"lsb_release -cs"
msgstr ""
"Показване ID на линукс дистрибутора\n"
"lsb_release -is\n"
"\n"
"Показване номера на Linux версията\n"
"lsb_release -rs\n"
"\n"
"Показва кодово име на Linux\n"
"lsb_release -cs"

#: ../ailurus/common/tips.py:41
msgid ""
"Display machine hardware name\n"
"uname -m"
msgstr ""
"Показване на хардуерното име на машината\n"
"uname -m"

#: ../ailurus/common/tips.py:44
msgid ""
"List all PCI devices, such as display card and ethernet card.\n"
"lspci\n"
msgstr ""
"Показва всички PCI устройства, като видео карта и мрежова карта.\n"
"lspci\n"

#: ../ailurus/common/tips.py:48
msgid ""
"Reclaim memory which stores pagecache, dentries and inodes\n"
"echo 3 > /proc/sys/vm/drop_caches\n"
msgstr ""
"Изискване на паметта, която записва кеша на страници, d-записи и inodes\n"
"echo 3 > /proc/sys/vm/drop_caches\n"

#: ../ailurus/common/tips.py:52
msgid ""
"Display a list of modules in the Linux Kernel\n"
"lsmod\n"
msgstr ""
"Показва списък с модули в Linux ядрото\n"
"lsmod\n"

#: ../ailurus/common/tips.py:56
msgid ""
"List USB devices\n"
"lsusb -v\n"
msgstr ""
"Списък с USB устройствата\n"
"lsusb -v\n"

#: ../ailurus/common/tips.py:60
msgid ""
"Display the status of ethernet card\n"
"sudo ethtool eth0\n"
msgstr ""
"Поакзва състоянието на мрежовата карта\n"
"sudo ethtool eth0\n"

#: ../ailurus/common/tips.py:64
msgid ""
"List hardware\n"
"sudo lshw\n"
msgstr ""
"Списък на хардуера\n"
"sudo lshw\n"

#: ../ailurus/common/tips.py:68
msgid ""
"List harddisk partitions\n"
"sudo fdisk -l\n"
msgstr ""
"Списък с дяловете на диска\n"
"sudo fdisk -l\n"

#: ../ailurus/common/tips.py:72
msgid ""
"Display SATA harddisk parameters\n"
"sudo hdparm -I /dev/sda\n"
msgstr ""
"Показване на параметрите за SATA твърди дискове\n"
"sudo hdparm -I /dev/sda\n"

#: ../ailurus/common/tips.py:76
msgid ""
"Display disk space usage\n"
"df -h\n"
msgstr ""
"Показва, колко място е използвано от твърдия диск\n"
"df -h\n"

#: ../ailurus/common/tips.py:80
msgid ""
"Display file/folder space usage\n"
"du -bsh FOLDER_NAME\n"
msgstr ""
"Показване наизползваното пространство от файл/директория\n"
"du -bsh FOLDER_NAME\n"

#: ../ailurus/common/tips.py:84
msgid ""
"Display amount of free and used memory\n"
"free\n"
msgstr ""
"Показва количеството използвана и свободна памет\n"
"free\n"

#: ../ailurus/common/tips.py:88
msgid ""
"Display processes\n"
"ps -e\n"
"Display a tree of processes\n"
"pstree\n"
"Display processes dynamically\n"
"top\n"
msgstr ""
"Показване на процеси\n"
"ps -e\n"
"Показване на дърво от процеси\n"
"pstree\n"
"Динамично показване на процеси\n"
"top\n"

#: ../ailurus/common/tips.py:96
msgid ""
"Terminate a process with a given process id\n"
"sudo kill -9 PROCESS_ID\n"
msgstr ""
"Прекратяване на процес по дадено ID на процесора\n"
"sudo kill -9 PROCESS_ID\n"

#: ../ailurus/common/tips.py:100
msgid ""
"Terminate all processes with a given name\n"
"sudo killall PROCESS_NAME\n"
msgstr ""
"Прекратяване на всички процеси с дадено име\n"
"sudo killall PROCESS_NAME\n"

#: ../ailurus/common/tips.py:104
msgid ""
"List files which are opened by a given process\n"
"lsof -p PROCESS_ID\n"
"lsof -c PROCESS_NAME\n"
msgstr ""
"Показване на списък с всички файлове, отворени от даден процес\n"
"lsof -p PROCESS_ID\n"
"lsof -c PROCESS_NAME\n"

#: ../ailurus/common/tips.py:109
msgid ""
"List processes which opened a given file\n"
"lsof FILE_NAME\n"
msgstr ""
"Списък с процесите, които са отворили даден файл\n"
"lsof FILE_NAME\n"

#: ../ailurus/common/tips.py:113
msgid ""
"List processes which are using port 80\n"
"lsof -i :80\n"
msgstr ""
"Списък с процесите, които използват порт 80\n"
"lsof -i :80\n"

#: ../ailurus/common/tips.py:117
msgid ""
"Configure an ADSL connection\n"
"sudo pppoeconf\n"
msgstr ""
"Настройка на ADSL връзка\n"
"sudo pppoeconf\n"

#: ../ailurus/common/tips.py:121
msgid ""
"Starts up ADSL connections\n"
"sudo pon\n"
"Shuts down ADSL connections\n"
"sudo poff\n"
msgstr ""
"Стартира ADSL връзки\n"
"sudo pon\n"
"Спира ADSL връзки\n"
"sudo poff\n"

#: ../ailurus/common/tips.py:127
msgid ""
"Display MAC of a given IP address\n"
"arping IP_ADDRESS\n"
msgstr ""
"Показва MAC на даден IP адрес\n"
"arping IP_ADDRESS\n"

#: ../ailurus/common/tips.py:131
msgid ""
"Display NetBIOS name of a given IP address\n"
"nmblookup -A IP_ADDRESS\n"
msgstr ""
"Показва NetBIOS името на даден IP адрес\n"
"nmblookup -A IP_ADDRESS\n"

#: ../ailurus/common/tips.py:135
msgid ""
"Display IP address and MAC\n"
"ifconfig -a\n"
msgstr ""
"Показва IP aдрес и MAC\n"
"ifconfig -a\n"

#: ../ailurus/common/tips.py:139
msgid ""
"Display route\n"
"netstat -rn\n"
msgstr ""
"Показва маршрут\n"
"netstat -rn\n"

#: ../ailurus/common/tips.py:143
msgid ""
"Set MAC of ethernet interface\n"
"sudo ifconfig eth0 hw ether 00:11:22:33:44:55\n"
msgstr ""
"Задава MAC за мрежови интерфейс\n"
"sudo ifconfig eth0 hw ether 00:11:22:33:44:55\n"

#: ../ailurus/common/tips.py:147
msgid ""
"Display information of a domain name\n"
"whois example.com\n"
msgstr ""
"Показва информация за домейн\n"
"whois example.com\n"

#: ../ailurus/common/tips.py:151
msgid ""
"Display the network path to a given host\n"
"tracepath example.com\n"
msgstr ""
"Показване на пътя до даден хост в мрежата\n"
"tracepath example.com\n"

#: ../ailurus/common/tips.py:155
msgid ""
"Request an IP address from DHCP server\n"
"sudo dhclient\n"
msgstr ""
"Иска IP адрес от DHCP сървър\n"
"sudo dhclient\n"

#: ../ailurus/common/tips.py:159
msgid ""
"Temporarily restart an init script\n"
"sudo /etc/init.d/SCRIPT_NAME restart\n"
"Temporarily stop an init script\n"
"sudo /etc/init.d/SCRIPT_NAME stop\n"
msgstr ""
"Временно рестартиране на init скрипт\n"
"sudo /etc/init.d/SCRIPT_NAME restart\n"
"Временно спиране на init скрипт\n"
"sudo /etc/init.d/SCRIPT_NAME stop\n"

#: ../ailurus/common/tips.py:165
msgid ""
"Add a user\n"
"sudo adduser USER_NANE\n"
"Delete a user\n"
"sudo deluser USER_NAME\n"
msgstr ""
"Добавяне на потребител\n"
"sudo adduser USER_NANE\n"
"Изтриване на потребител\n"
"sudo deluser USER_NAME\n"

#: ../ailurus/common/tips.py:171
msgid ""
"Change user password\n"
"sudo passwd USER_NAME\n"
msgstr ""
"Смяна на паролата на потребител\n"
"sudo passwd USER_NAME\n"

#: ../ailurus/common/tips.py:175
msgid ""
"Changes user fullname, office number, office extension, and home phone "
"number information.\n"
"sudo chfn USER_NAME\n"
"Display user information\n"
"finger USER_NAME\n"
msgstr ""
"Промена информация като пълно име, офис телефон, офист разширение и домашен "
"телефон за потребител.\n"
"sudo chfn USER_NAME\n"
"Показва информация за потребител\n"
"finger USER_NAME\n"

#: ../ailurus/common/tips.py:181
msgid ""
"Temporarily prevent a user from logging in\n"
"sudo usermod -L USER_NAME\n"
"Revoke the operation above\n"
"sudo usermod -U USER_NAME\n"
msgstr ""
"Временно блокиране на потребител от влизане в системата\n"
"sudo usermod -L USER_NAME\n"
"Анулира горната команда\n"
"sudo usermod -U USER_NAME\n"

#: ../ailurus/common/tips.py:187
msgid ""
"Add a user to admin group\n"
"sudo usermod -G admin -a USER_NAME\n"
msgstr ""
"Добавя потребител в групата admin\n"
"sudo usermod -G admin -a USER_NAME\n"

#: ../ailurus/common/tips.py:191
msgid ""
"Set the HTTP proxy\n"
"export http_proxy=http://PROXY.DOMAIN.NAME:PORT\n"
msgstr ""
"Задаване на HTTP прокси\n"
"export http_proxy=http://PROXY.DOMAIN.NAME:PORT\n"

#: ../ailurus/common/tips.py:195
msgid ""
"Modify the information displayed after logging in\n"
"sudo vim /etc/motd.tail\n"
msgstr ""
"Промяна на информацията, показване след вход в системата\n"
"sudo vim /etc/motd.tail\n"

#: ../ailurus/common/tips.py:199
msgid ""
"Choose the input method for X Window\n"
"im-switch -c\n"
msgstr ""
"Изберете метода за въвеждане за X прозорец\n"
"im-switch -c\n"

#: ../ailurus/common/tips.py:203
msgid ""
"Convert the file name from GBK to UTF8\n"
"convmv -r -f gbk -t utf8 --notest FILE_NAME\n"
msgstr ""
"Конвертира име на файл от GBK на UTF8\n"
"convmv -r -f gbk -t utf8 --notest FILE_NAME\n"

#: ../ailurus/common/tips.py:207
msgid ""
"Convert the file content from GBK to UTF8\n"
"iconv -f gbk -t utf8 FILE_NAME\n"
msgstr ""
"Конвертира съдържанието на файл от GBK на UTF8\n"
"iconv -f gbk -t utf8 FILE_NAME\n"

#: ../ailurus/common/tips.py:211
msgid ""
"Convert tags in '*.mp3' from GBK to UTF8\n"
"find . -name '*.mp3' -execdir mid3iconv -e GBK {} \\;\n"
msgstr ""
"Конвертира тагове в \"*.mp3\" от GBK на UTF8\n"
"find . -name '*.mp3' -execdir mid3iconv -e GBK {} \\;\n"

#: ../ailurus/common/tips.py:215
msgid ""
"Read a long file\n"
"less FILE_NAME\n"
msgstr ""
"Прочита дълъг файл\n"
"less FILE_NAME\n"

#: ../ailurus/common/tips.py:219
msgid ""
"Print lines matching a pattern\n"
"grep REG_EXP FILE_NAME\n"
msgstr ""
"Показва редове, отговарящи на даден модел (RegEx)\n"
"grep REG_EXP FILE_NAME\n"

#: ../ailurus/common/tips.py:223
msgid ""
"Display a list of file name. The files contain a given string.\n"
"grep -lr REG_EXP PATHNAME\n"
msgstr ""
"Показва списък с имена на файлове, които съдържат дадена дума или регулярен "
"израз.\n"
"grep -lr REG_EXP PATHNAME\n"

#: ../ailurus/common/tips.py:227
msgid ""
"Display all '.txt' file\n"
"find . -name '*.txt'\n"
msgstr ""
