msgid ""
"Display SATA harddisk parameters\n"
"sudo hdparm -I /dev/sda\n"
msgstr ""
"Zobrazení parametrů SATA pevného disku\n"
"sudo hdparm -I /dev/sda\n"

#: ../ailurus/common/tips.py:76
msgid ""
"Display disk space usage\n"
"df -h\n"
msgstr ""
"Zobrazení využití místa na disku\n"
"df -h\n"

#: ../ailurus/common/tips.py:80
msgid ""
"Display file/folder space usage\n"
"du -bsh FOLDER_NAME\n"
msgstr ""
"Zobrazení velikosti souboru/složky\n"
"du -bsh NÁZEV_SLOŽKY\n"

#: ../ailurus/common/tips.py:84
msgid ""
"Display amount of free and used memory\n"
"free\n"
msgstr ""
"Zobrazení množství volné a používané paměti\n"
"free\n"

#: ../ailurus/common/tips.py:88
msgid ""
"Display processes\n"
"ps -e\n"
"Display a tree of processes\n"
"pstree\n"
"Display processes dynamically\n"
"top\n"
msgstr ""
"Zobrazení procesů\n"
"ps -e\n"
"Zobrazení stromu procesů\n"
"pstree\n"
"Dynamické zobrazení procesů\n"
"top\n"

