#: ../ailurus/common/tips.py:28
msgid ""
"motto of Ailurus\n"
"Give a man a fish and he can eat for a day; but if you teach him how to "
"fish, he'll eat for a lifetime."
msgstr ""
"Motto Ailurus\n"
"Daj človeku rybu a môže jesť celý deň, ale ak ho naučíte ako chytať ryby, "
"bude mať čo jesť po celý život."

#: ../ailurus/common/tips.py:31
msgid ""
"Display Linux distributor's ID\n"
"lsb_release -is\n"
"\n"
"Display Linux release number\n"
"lsb_release -rs\n"
"\n"
"Display Linux code name\n"
"lsb_release -cs"
msgstr ""
"Zobraziť ID linuxovej distribúcie\n"
"lsb_release -is\n"
"\n"
"Zobraziť číslo vydania linuxu\n"
"lsb_release -rs\n"
"\n"
"Zobraziť kódový názov linuxu\n"
"lsb_release -cs"

#: ../ailurus/common/tips.py:41
msgid ""
"Display machine hardware name\n"
"uname -m"
msgstr ""
"Zobraziť názov hardvérového stroja\n"
"uname -m"

#: ../ailurus/common/tips.py:44
msgid ""
"List all PCI devices, such as display card and ethernet card.\n"
"lspci\n"
msgstr ""
"Zoznam všetkých PCI zariadení, ako je grafická a sieťová karta.\n"
"lspci\n"

#: ../ailurus/common/tips.py:48
msgid ""
"Reclaim memory which stores pagecache, dentries and inodes\n"
"echo 3 > /proc/sys/vm/drop_caches\n"
msgstr ""
"Uvoľniť pamäť, ktorá ukladá stránkovaciu pamäť, dentries a uzlov.\n"
"echo 3 > /proc/sys/vm/drop_caches\n"

#: ../ailurus/common/tips.py:52
msgid ""
"Display a list of modules in the Linux Kernel\n"
"lsmod\n"
msgstr ""
"Zobraziť zoznam modulov v Linuxovom jadre\n"
"lsmod\n"

#: ../ailurus/common/tips.py:56
msgid ""
"List USB devices\n"
"lsusb -v\n"
msgstr ""
"Zoznam USB zariadení\n"
"lsusb -v\n"

#: ../ailurus/common/tips.py:60
msgid ""
"Display the status of ethernet card\n"
"sudo ethtool eth0\n"
msgstr ""
"Zobraziť stav sieťovej karty\n"
"sudo ethtool eth0\n"

#: ../ailurus/common/tips.py:64
msgid ""
"List hardware\n"
"sudo lshw\n"
msgstr ""
"Zoznam hardvéru\n"
"sudo lshw\n"

#: ../ailurus/common/tips.py:68
msgid ""
"List harddisk partitions\n"
"sudo fdisk -l\n"
msgstr ""
"Zoznam harddiskových partícií\n"
"sudo fdisk -l\n"

#: ../ailurus/common/tips.py:72
msgid ""
"Display SATA harddisk parameters\n"
"sudo hdparm -I /dev/sda\n"
msgstr ""
"Zobraziť parametre SATA harddisku\n"
"sudo hdparm -I /dev/sda\n"

#: ../ailurus/common/tips.py:76
msgid ""
"Display disk space usage\n"
"df -h\n"
msgstr ""
"Zobraziť využitie disku\n"
"df -h\n"

#: ../ailurus/common/tips.py:80
msgid ""
"Display file/folder space usage\n"
"du -bsh FOLDER_NAME\n"
msgstr ""
"Zobrazenie využitia miesta súborov/priečinkov\n"
"du -bsh FOLDER_NAME\n"

#: ../ailurus/common/tips.py:84
msgid ""
"Display amount of free and used memory\n"
"free\n"
msgstr ""
"Zobrazenie množstva voľnej a používanej pamäte\n"
"free\n"

#: ../ailurus/common/tips.py:88
msgid ""
"Display processes\n"
"ps -e\n"
"Display a tree of processes\n"
"pstree\n"
"Display processes dynamically\n"
"top\n"
msgstr ""
"Zobraziť procesy\n"
"ps -e\n"
"Zobraziť strom procesov\n"
"pstree\n"
"Zobraziť dynamické procesy\n"
"top\n"

#: ../ailurus/common/tips.py:96
msgid ""
"Terminate a process with a given process id\n"
"sudo kill -9 PROCESS_ID\n"
msgstr ""
"Ukončiť proces daného ID procesu\n"
"sudo kill -9 PROCESS_ID\n"

#: ../ailurus/common/tips.py:100
msgid ""
"Terminate all processes with a given name\n"
"sudo killall PROCESS_NAME\n"
msgstr ""
"Ukončiť všetky procesy s uvedeným názvom\n"
"sudo killall PROCESS_NAME\n"

#: ../ailurus/common/tips.py:104
msgid ""
"List files which are opened by a given process\n"
"lsof -p PROCESS_ID\n"
"lsof -c PROCESS_NAME\n"
msgstr ""
"Zoznam súborov, ktoré sú otvorené na základe určitého procesu\n"
"lsof -p PROCESS_ID\n"
"lsof -c PROCESS_NAME\n"

#: ../ailurus/common/tips.py:109
msgid ""
"List processes which opened a given file\n"
"lsof FILE_NAME\n"
msgstr "Zoznam procesov, ktoré otvára daný súbor\n"

#: ../ailurus/common/tips.py:113
msgid ""
"List processes which are using port 80\n"
"lsof -i :80\n"
msgstr ""
"Zoznam procesov, ktoré používajú port 80\n"
"lsof -i :80\n"

#: ../ailurus/common/tips.py:117
msgid ""
"Configure an ADSL connection\n"
"sudo pppoeconf\n"
msgstr ""
"Konfigurovať ADSL pripojenie\n"
"sudo pppoeconf\n"

#: ../ailurus/common/tips.py:121
msgid ""
"Starts up ADSL connections\n"
"sudo pon\n"
"Shuts down ADSL connections\n"
"sudo poff\n"
msgstr ""
"Spustiť ADSL pripojenie\n"
"sudo pon\n"
"Vypnúť ADSL pripojenie\n"
"sudo poff\n"

#: ../ailurus/common/tips.py:127
msgid ""
"Display MAC of a given IP address\n"
"arping IP_ADDRESS\n"
msgstr ""
"Zobrazenie MAC danej IP adresy\n"
"arping IP_ADDRESS\n"

#: ../ailurus/common/tips.py:131
msgid ""
"Display NetBIOS name of a given IP address\n"
"nmblookup -A IP_ADDRESS\n"
msgstr ""
"Zobraziť názov NetBIOS danej IP adresy\n"
"nmblookup -A IP_ADDRESS\n"

#: ../ailurus/common/tips.py:135
msgid ""
"Display IP address and MAC\n"
"ifconfig -a\n"
msgstr ""
"Zobraziť IP a MAC  adresu\n"
"ifconfig -a\n"

#: ../ailurus/common/tips.py:139
msgid ""
"Display route\n"
"netstat -rn\n"
msgstr ""
"Zobraziť trasu\n"
"netstat -rn\n"

#: ../ailurus/common/tips.py:143
msgid ""
"Set MAC of ethernet interface\n"
"sudo ifconfig eth0 hw ether 00:11:22:33:44:55\n"
msgstr ""
"Nastaviť MAC rozhranie Ethernetu\n"
"sudo ifconfig eth0 hw ether 00:11:22:33:44:55\n"

#: ../ailurus/common/tips.py:147
msgid ""
"Display information of a domain name\n"
"whois example.com\n"
msgstr ""
"Zobraziť informácie o názve domény\n"
"whois example.com\n"

#: ../ailurus/common/tips.py:151
msgid ""
"Display the network path to a given host\n"
"tracepath example.com\n"
msgstr ""
"Zobraziť sieťovú cestu k danému počítaču\n"
"tracepath example.com\n"

#: ../ailurus/common/tips.py:155
msgid ""
"Request an IP address from DHCP server\n"
"sudo dhclient\n"
msgstr ""
"Požiadať o IP adresu z DHCP servera\n"
"sudo dhclient\n"

#: ../ailurus/common/tips.py:159
msgid ""
"Temporarily restart an init script\n"
"sudo /etc/init.d/SCRIPT_NAME restart\n"
"Temporarily stop an init script\n"
"sudo /etc/init.d/SCRIPT_NAME stop\n"
msgstr ""
"Dočasný reštart init skriptu\n"
"sudo /etc/init.d/SCRIPT_NAME restart\n"
"Dočasné zastavenie init skriptu\n"
"sudo /etc/init.d/SCRIPT_NAME stop\n"

#: ../ailurus/common/tips.py:165
msgid ""
"Add a user\n"
"sudo adduser USER_NANE\n"
"Delete a user\n"
"sudo deluser USER_NAME\n"
msgstr ""
"Pridať používateľa\n"
"sudo adduser USER_NANE\n"
"Odstrániť používateľa\n"
"sudo deluser USER_NAME\n"

#: ../ailurus/common/tips.py:171
msgid ""
"Change user password\n"
"sudo passwd USER_NAME\n"
msgstr ""
"Zmeniť heslo používateľa\n"
"sudo passwd USER_NAME\n"

#: ../ailurus/common/tips.py:175
msgid ""
"Changes user fullname, office number, office extension, and home phone "
"number information.\n"
"sudo chfn USER_NAME\n"
"Display user information\n"
"finger USER_NAME\n"
msgstr ""
"Zmeny užívateľského názvu, informácie o kancelárii a o domácom telefónnom "
"čísle.\n"
"sudo chfn USER_NAME\n"
"Zobraziť používateľské informácie\n"
"finger USER_NAME\n"

