msgid ""
"Terminate a process with a given process id\n"
"sudo kill -9 PROCESS_ID\n"
msgstr ""
"Ukončení procesu s daným id procesu\n"
"sudo kill -9 ID_PROCESU\n"

#: ../ailurus/common/tips.py:100
msgid ""
"Terminate all processes with a given name\n"
"sudo killall PROCESS_NAME\n"
msgstr ""
"Ukončení procesů s daným názvem\n"
"sudo killall NÁZEV_PROCESŮ\n"

#: ../ailurus/common/tips.py:104
msgid ""
"List files which are opened by a given process\n"
"lsof -p PROCESS_ID\n"
"lsof -c PROCESS_NAME\n"
msgstr ""
"Seznam souborů, které jsou otevřeny na základě určitého procesu\n"
"lsof -p ID_PROCESU\n"
"lsof -c NÁZEV_PROCESU\n"

#: ../ailurus/common/tips.py:109
msgid ""
"List processes which opened a given file\n"
"lsof FILE_NAME\n"
msgstr ""
"Seznam procesů, který byl otevřen daným souborem\n"
"lsof NÁZEV_SOUBORU\n"

