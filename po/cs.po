#: ../ailurus/common/tips.py:31
msgid ""
"Display Linux distributor's ID\n"
"lsb_release -is\n"
"\n"
"Display Linux release number\n"
"lsb_release -rs\n"
"\n"
"Display Linux code name\n"
"lsb_release -cs"
msgstr ""
"Zobrazení ID linuxové distribuce\n"
"lsb_release -is\n"
"\n"
"Zobrazení čísla vydání\n"
"lsb_release -rs\n"
"\n"
"Zobrazení kódového označení\n"
"lsb_release -cs"

#: ../ailurus/common/tips.py:41
msgid ""
"Display machine hardware name\n"
"uname -m"
msgstr ""
"Zobrazit název architektury stroje\n"
"uname -m"

#: ../ailurus/common/tips.py:44
msgid ""
"List all PCI devices, such as display card and ethernet card.\n"
"lspci\n"
msgstr ""
"Seznam všech PCI zařízení, např. jako je zobrazení síťové karty.\n"
"lspci\n"

#: ../ailurus/common/tips.py:48
msgid ""
"Reclaim memory which stores pagecache, dentries and inodes\n"
"echo 3 > /proc/sys/vm/drop_caches\n"
msgstr ""
"Uvolnění paměti, která se ukládá do pagecache, dentries a inodes\n"
"echo 3 > /proc/sys/vm/drop_caches\n"

#: ../ailurus/common/tips.py:52
msgid ""
"Display a list of modules in the Linux Kernel\n"
"lsmod\n"
msgstr ""
"Zobrazení seznamu modulů v linuxovém jádře.\n"
"lsmod\n"

#: ../ailurus/common/tips.py:56
msgid ""
"List USB devices\n"
"lsusb -v\n"
msgstr ""
"Seznam USB zařízení\n"
"lsusb -v\n"

#: ../ailurus/common/tips.py:60
msgid ""
"Display the status of ethernet card\n"
"sudo ethtool eth0\n"
msgstr ""
"Zobrazí stav síťové karty\n"
"sudo ethtool eth0\n"

#: ../ailurus/common/tips.py:64
msgid ""
"List hardware\n"
"sudo lshw\n"
msgstr ""
"Seznam hardwaru\n"
"sudo lshw\n"

#: ../ailurus/common/tips.py:68
msgid ""
"List harddisk partitions\n"
"sudo fdisk -l\n"
msgstr ""
"Seznam oddílů na pevném disku\n"
"sudo fdisk -l\n"

