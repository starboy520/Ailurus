msgid ""
"Display NetBIOS name of a given IP address\n"
"nmblookup -A IP_ADDRESS\n"
msgstr ""
"Zobrazení názvu NetBIOS dané IP adresy\n"
"nmblookup -A IP_ADRESA\n"

#: ../ailurus/common/tips.py:135
msgid ""
"Display IP address and MAC\n"
"ifconfig -a\n"
msgstr ""
"Zobrazení IP adresy a MAC\n"
"ifconfig -a\n"

#: ../ailurus/common/tips.py:139
msgid ""
"Display route\n"
"netstat -rn\n"
msgstr ""
"Zobrazení trasy\n"
"netstat -rn\n"

