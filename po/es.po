"marcas de tiempo. \n"
"cp -a DIRECTORIO_ORIGEN DIRECTORIO_DESTINO\n"

#: ../ailurus/common/tips.py:264
msgid ""
"Determine file type\n"
"file FILE_NAME\n"
msgstr ""
"Determinar el tipo de archivo\n"
"file NOMBRE_DE_ARCHIVO\n"

#: ../ailurus/common/tips.py:268
msgid ""
"Output the last 6 lines\n"
"tail -n 6 FILE_NAME\n"
msgstr ""
"Mostrar en pantalla las últimas 6 líneas\n"
"tail -n 6 NOMBRE_DE_ARCHIVO\n"

#: ../ailurus/common/tips.py:272
msgid ""
"Copy files via SSH\n"
"scp -rp FILE_NAME USERNAME@HOST:DEST_PATH\n"
msgstr ""
"Copiar archivos vía SSH\n"
"scp -rp NOMBRE_DE_ARCHIVO NOMBRE_DE_USUARIO@HOST:RUTA_DE_DESTINO\n"

#: ../ailurus/common/tips.py:276
msgid ""
"Rename '*.rm' files to '*.rmvb' files\n"
"rename 's/.rm$/.rmvb/' *\n"
msgstr ""
"Renombrar los archivos '*.rm' a '*.rmvb'\n"
"rename 's/.rm$/.rmvb/' *\n"

#: ../ailurus/common/tips.py:280
msgid ""
"Change the file name to lowercase\n"
"rename 'tr/A-Z/a-z/' *\n"
msgstr ""
"Cambia el nombre de archivo a minúsculas\n"
"rename 'tr/A-Z/a-z/' *\n"

#: ../ailurus/common/tips.py:284
msgid ""
"Display subdirectories in current directory\n"
"ls -d */.\n"
msgstr ""
"Mostrar los sub-directorios en el directorio actual\n"
"ls -d */.\n"

#: ../ailurus/common/tips.py:288
msgid ""
"Display file number in current directory\n"
"ls . | wc -w\n"
msgstr ""
"Mostrar el número de archivo en el directorio actual\n"
"ls . | wc -w\n"

#: ../ailurus/common/tips.py:292
msgid ""
"Extract \"*.gz\" file\n"
"gunzip FILE_NAME.gz\n"
"Extract \"*.tar.gz\" file\n"
"tar zxf FILE_NAME.tar.gz\n"
"Extract \"*.tar.bz2\" file\n"
"tar jxf FILE_NAME.tar.bz2\n"
msgstr ""
"Extraer un archivo «*.gz»\n"
"gunzip NOMBRE_DE_ARCHIVO.gz\n"
"Extraer un archivo «*.tar.gz»\n"
"tar zxf NOMBRE_DE_ARCHIVO.tar.gz\n"
"Extraer un archivo «*.tar.bz2»\n"
"tar jxf NOMBRE_DE_ARCHIVO.tar.bz2\n"

#: ../ailurus/common/tips.py:300
msgid ""
"Do compression\n"
"tar czf FILE_NAME.tar.gz FILE1 FILE2 FILE3\n"
"tar cjf FILE_NAME.tar.bz2 FILE1 FILE2 FILE3\n"
msgstr ""
"Comprimir\n"
"tar czf NOMBRE_DE_ARCHIVO.tar.gz ARCHIVO_1 ARCHIVO_2 ... ARCHIVO_N\n"
"tar cjf NOMBRE_DE_ARCHIVO.tar.bz2 ARCHIVO_1 ARCHIVO_2 ... ARCHIVO_N\n"

#: ../ailurus/common/tips.py:305
msgid ""
"Displays a calendar\n"
"cal\n"
"cal MONTH YEAR\n"
msgstr ""
"Mostrar un calendario\n"
"cal\n"
"cal MES AÑO\n"

#: ../ailurus/common/tips.py:310
msgid ""
"Set the date and time via NTP\n"
"sudo ntpdate ntp.ubuntu.com\n"
msgstr ""
"Configurar fecha y hora vía NTP\n"
"sudo ntpdate ntp.ubuntu.com\n"

#: ../ailurus/common/tips.py:314
msgid ""
"Poweroff your computer\n"
"sudo halt\n"
"sudo shutdown -h now\n"
"Poweroff your computer in 23:00\n"
"sudo shutdown -h 23:00\n"
"Poweroff your computer after 60 minutes\n"
"sudo shutdown -h +60\n"
msgstr ""
"Apagar su computadora\n"
"sudo halt\n"
"sudo shutdown -h now\n"
"Apagar su computadora a las 23:00\n"
"sudo shutdown -h 23:00\n"
"Apagar su computadora después de 60 minutos\n"
"sudo shutdown -h +60\n"

#: ../ailurus/common/tips.py:323
msgid ""
"Reboot your computer\n"
"sudo reboot\n"
"sudo shutdown -r now\n"
msgstr ""
"Reiniciar su computadora\n"
"sudo reboot\n"
"sudo shutdown -r now\n"

#: ../ailurus/common/tips.py:328
msgid ""
"If you want some program to start up automatically, please put '.desktop' "
"files into '~/.config/autostart'\n"
msgstr ""
"Sí desea que algún programa se inicie automáticamente, coloque los archivos "
"'.desktop' en '~/.config/autostart'\n"

